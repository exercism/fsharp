"scale-generator.8th" f:include
needs exercism/test
with: test
17 tests

"Chromatic scale with sharps"
    expect_equal(chromatic "C", c("C", "C#", "D", "D#", "E", "F", "F#", "G", "G#", "A", "A#"
    "B"
    equal?)

SKIP-REST-OF-TESTS

"Chromatic scale with flats"
    expect_equal(chromatic "F", c("F", "Gb", "G", "Ab", "A", "Bb", "B", "C", "Db", "D", "Eb"
    "E"
    equal?)

"Simple major scale"
    expect_equal(interval "MMmMMMm" "C", c("C", "D", "E", "F", "G", "A", "B"
    "C"
    equal?)

"Major scale with sharps"
    expect_equal(interval "MMmMMMm" "G", c("G", "A", "B", "C", "D", "E", "F#"
    "G"
    equal?)

"Major scale with flats"
    expect_equal(interval "MMmMMMm" "F", c("F", "G", "A", "Bb", "C", "D", "E"
    "F"
    equal?)

"Minor scale with sharps"
    expect_equal(interval "MmMMmMM" "f#", c("F#", "G#", "A", "B", "C#", "D", "E"
    "F#"
    equal?)

"Minor scale with flats"
    expect_equal(interval "MmMMmMM" "bb", c("Bb", "C", "Db", "Eb", "F", "Gb", "Ab"
    "Bb"
    equal?)

"Dorian mode"
    expect_equal(interval "MmMMMmM" "d", c("D", "E", "F", "G", "A", "B", "C"
    "D"
    equal?)

"Mixolydian mode"
    expect_equal(interval "MMmMMmM" "Eb", c("Eb", "F", "G", "Ab", "Bb", "C", "Db"
    "Eb"
    equal?)

"Lydian mode"
    expect_equal(interval "MMMmMMm" "a", c("A", "B", "C#", "D#", "E", "F#", "G#"
    "A"
    equal?)

"Phrygian mode"
    expect_equal(interval "mMMMmMM" "e", c("E", "F", "G", "A", "B", "C", "D"
    "E"
    equal?)

"Locrian mode"
    expect_equal(interval "mMMmMMM" "g", c("G", "Ab", "Bb", "C", "Db", "Eb", "F"
    "G"
    equal?)

"Harmonic minor"
    expect_equal(interval "MmMMmAm" "d", c("D", "E", "F", "G", "A", "Bb", "Db"
    "D"
    equal?)

"Octatonic"
    expect_equal(interval "MmMmMmMm" "C", c("C", "D", "D#", "F", "F#", "G#", "A", "B"
    "C"
    equal?)

"Hexatonic"
    expect_equal(interval "MMMMMM" "Db", c("Db", "Eb", "F", "G", "A", "B"
    "Db"
    equal?)

"Pentatonic"
    expect_equal(interval "MMAMA" "A", c("A", "B", "C#", "E", "F#"
    "A"
    equal?)

"Enigmatic"
    expect_equal(interval "mAMMMmm" "G", c("G", "G#", "B", "C#", "D#", "F", "F#"
    "G"
    equal?)

end-of-tests
;with
