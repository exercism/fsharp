"satellite.8th" f:include
needs exercism/test
with: test
6 tests

"Empty tree"
  expected: Result<Tree,string> = Ok (
        Empty
    )
  expect_equal(treeFromTraversals c(] [), expected)

SKIP-REST-OF-TESTS

"Tree with one item"
  expected: Result<Tree,string> = Ok (
        Node("a", Empty, Empty)
    )
  expect_equal(treeFromTraversals c("a") c("a"), expected)

"Tree with many items"
  expected: Result<Tree,string> = Ok (
        Node(
            "a",
            Node("i", Empty, Empty),
            Node(
                "x",
                Node("f", Empty, Empty),
                Node("r", Empty, Empty)
            )
        )
    )
    expect_equal(treeFromTraversals c("i", "a", "f", "x", "r") c("a", "i", "x", "f"
    "r"
    equal?, expected)

"Reject traversals of different length"
  expected: Result<Tree,string> = Error "traversals must have the same length"
    expect_equal(treeFromTraversals c("b", "a", "r") c("a"
    "b"
    equal?, expected)

"Reject inconsistent traversals of same length"
  expected: Result<Tree,string> = Error "traversals must have the same elements"
    expect_equal(treeFromTraversals c("a", "b", "c") c("x", "y"
    "z"
    equal?, expected)

"Reject traversals with repeated items"
  expected: Result<Tree,string> = Error "traversals must contain unique items"
    expect_equal(treeFromTraversals c("b", "a", "a") c("a", "b"
    "a"
    equal?, expected)
