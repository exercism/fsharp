~ "scrabble-score.ua" ~ ScrabbleScore

# Lowercase letter
⍤⤙≍ 1 score "a"

# Uppercase letter
⍤⤙≍ 1 score "A"

# Valuable letter
⍤⤙≍ 4 score "f"

# Short word
⍤⤙≍ 2 score "at"

# Short, valuable word
⍤⤙≍ 12 score "zoo"

# Medium word
⍤⤙≍ 6 score "street"

# Medium, valuable word
⍤⤙≍ 22 score "quirky"

# Long, mixed-case word
⍤⤙≍ 41 score "OxyphenButazone"

# English-like word
⍤⤙≍ 8 score "pinata"

# Empty input
⍤⤙≍ 0 score ""

# Entire alphabet available
⍤⤙≍ 87 score "abcdefghijklmnopqrstuvwxyz"

