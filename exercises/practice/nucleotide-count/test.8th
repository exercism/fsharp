"nucleotide-count.8th" f:include
needs exercism/test
with: test
5 tests

"Empty strand"
  strand <- ""
  expected <- 
        [ ('A', 0);
          ('C', 0);
          ('G', 0);
          ('T', 0) ]
        |> Map.ofList
        |> Some
  expect_equal(nucleotideCounts(strand), expected)

SKIP-REST-OF-TESTS

"Can count one nucleotide in single-character input"
  strand <- "G"
  expected <- 
        [ ('A', 0);
          ('C', 0);
          ('G', 1);
          ('T', 0) ]
        |> Map.ofList
        |> Some
  expect_equal(nucleotideCounts(strand), expected)

"Strand with repeated nucleotide"
  strand <- "GGGGGGG"
  expected <- 
        [ ('A', 0);
          ('C', 0);
          ('G', 7);
          ('T', 0) ]
        |> Map.ofList
        |> Some
  expect_equal(nucleotideCounts(strand), expected)

"Strand with multiple nucleotides"
  strand <- "AGCTTTTCATTCTGACTGCAACGGGCAATATGTCTCTGTGTGGATTAAAAAAAGAGTGTCTGATAGCAGC"
  expected <- 
        [ ('A', 20);
          ('C', 12);
          ('G', 17);
          ('T', 21) ]
        |> Map.ofList
        |> Some
  expect_equal(nucleotideCounts(strand), expected)

"Strand with invalid nucleotides"
  strand <- "AGXXACT"
  expected <- None
  expect_equal(nucleotideCounts(strand), expected)
