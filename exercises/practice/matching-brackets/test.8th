"matching-brackets.8th" f:include
needs exercism/test
with: test
20 tests

"Paired square brackets"
  expect_true(isPaired "[]")

SKIP-REST-OF-TESTS

"Empty string"
  expect_true(isPaired "")

"Unpaired brackets"
  expect_false(isPaired "[[")

"Wrong ordered brackets"
  expect_false(isPaired "}{")

"Wrong closing bracket"
  expect_false(isPaired "{]")

"Paired with whitespace"
  expect_true(isPaired "{ }")

"Partially paired brackets"
  expect_false(isPaired "{[])")

"Simple nested brackets"
  expect_true(isPaired "{[]}")

"Several paired brackets"
  expect_true(isPaired "{}[]")

"Paired and nested brackets"
  expect_true(isPaired "(c({}({}[))])")

"Unopened closing brackets"
  expect_false(isPaired "{c())[]}")

"Unpaired and nested brackets"
  expect_false(isPaired "(c({))")

"Paired and wrong nested brackets"
  expect_false(isPaired "c(({)})")

"Paired and wrong nested brackets but innermost are correct"
  expect_false(isPaired "c(({}))")

"Paired and incomplete brackets"
  expect_false(isPaired "{}[")

"Too many closing brackets"
  expect_false(isPaired "c(])")

"Early unexpected brackets"
  expect_false(isPaired ")()")

"Early mismatched brackets"
  expect_false(isPaired "{)()")

"Math expression"
  expect_true(isPaired "(((185 + 223.85) * 15) - 543)/2")

"Complex latex expression"
  expect_true(isPaired "\left(\begin{array}{cc} \frac{1}{3} & x\\ \mathrm{e}^{x} &... x^2 \end{array}\right)")
