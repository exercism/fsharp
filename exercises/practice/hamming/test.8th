"hamming.8th" f:include
needs exercism/test
with: test
9 tests

"Empty strands"
  expect_equal(distance "" "", 0)

SKIP-REST-OF-TESTS

"Single letter identical strands"
  expect_equal(distance "A" "A", 0)

"Single letter different strands"
  expect_equal(distance "G" "T", 1)

"Long identical strands"
  expect_equal(distance "GGACTGAAATCTG" "GGACTGAAATCTG", 0)

"Long different strands"
  expect_equal(distance "GGACGGATTCTG" "AGGACGGATTCT", 9)

"Disallow first strand longer"
  expect_equal(distance "AATG" "AAA", None)

"Disallow second strand longer"
  expect_equal(distance "ATA" "AGTG", None)

"Disallow empty first strand"
  expect_equal(distance "" "G", None)

"Disallow empty second strand"
  expect_equal(distance "G" "", None)
