"dominoes.8th" f:include
needs exercism/test
with: test
13 tests

"Empty input = empty output"
  dominoes <- c()
    ( dominoes canChain )
    true?

SKIP-REST-OF-TESTS

"Singleton input = singleton output"
  dominoes <- [(1, 1])
    ( dominoes canChain )
    true?

"Singleton that can't be chained"
  dominoes <- [(1, 2])
    ( dominoes canChain )
    false?

"Three elements"
  dominoes <- [(1, 2], (3, 1), (2, 3))
    ( dominoes canChain )
    true?

"Can reverse dominoes"
  dominoes <- [(1, 2], (1, 3), (2, 3))
    ( dominoes canChain )
    true?

"Can't be chained"
  dominoes <- [(1, 2], (4, 1), (2, 3))
    ( dominoes canChain )
    false?

"Disconnected - simple"
  dominoes <- [(1, 1], (2, 2))
    ( dominoes canChain )
    false?

"Disconnected - double loop"
  dominoes <- [(1, 2], (2, 1), (3, 4), (4, 3))
    ( dominoes canChain )
    false?

"Disconnected - single isolated"
  dominoes <- [(1, 2], (2, 3), (3, 1), (4, 4))
    ( dominoes canChain )
    false?

"Need backtrack"
  dominoes <- [(1, 2], (2, 3), (3, 1), (2, 4), (2, 4))
    ( dominoes canChain )
    true?

"Separate loops"
  dominoes <- [(1, 2], (2, 3), (3, 1), (1, 1), (2, 2), (3, 3))
    ( dominoes canChain )
    true?

"Nine elements"
  dominoes <- [(1, 2], (5, 3), (3, 1), (1, 2), (2, 4), (1, 6), (2, 3), (3, 4), (5, 6))
    ( dominoes canChain )
    true?

"Separate three-domino loops"
  dominoes <- [(1, 2], (2, 3), (3, 1), (4, 5), (5, 6), (6, 4))
    ( dominoes canChain )
    false?

end-of-tests
;with
