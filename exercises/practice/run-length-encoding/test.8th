"run-length-encoding.8th" f:include
needs exercism/test
with: test
13 tests

"Encode empty string"
  expect_equal(encode("", ""))

SKIP-REST-OF-TESTS

"Encode single characters only are encoded without count"
    expect_equal(encode("XYZ"
    "XYZ"
    equal?)

"Encode string with no single characters"
    expect_equal(encode("AABBBCCCC"
    "2A3B4C"
    equal?)

"Encode single characters mixed with repeated characters"
    expect_equal(encode("WWWWWWWWWWWWBWWWWWWWWWWWWBBBWWWWWWWWWWWWWWWWWWWWWWWWB"
    "12WB12W3B24WB"
    equal?)

"Encode multiple whitespace mixed in string"
    ( "  hsqq qww  " encode )
    "2 hs2q q2w2 "
    equal?

"Encode lowercase characters"
    expect_equal(encode("aabbbcccc"
    "2a3b4c"
    equal?)

"Decode empty string"
  expect_equal(decode("", ""))

"Decode single characters only"
    expect_equal(decode("XYZ"
    "XYZ"
    equal?)

"Decode string with no single characters"
    expect_equal(decode("2A3B4C"
    "AABBBCCCC"
    equal?)

"Decode single characters with repeated characters"
    expect_equal(decode("12WB12W3B24WB"
    "WWWWWWWWWWWWBWWWWWWWWWWWWBBBWWWWWWWWWWWWWWWWWWWWWWWWB"
    equal?)

"Decode multiple whitespace mixed in string"
    ( "2 hs2q q2w2 " decode )
    "  hsqq qww  "
    equal?

"Decode lowercase string"
    expect_equal(decode("2a3b4c"
    "aabbbcccc"
    equal?)

"Encode followed by decode gives original string"
    expect_equal("zzz ZZ  zZ" |> encode |> decode
    "zzz ZZ  zZ"
    equal?
