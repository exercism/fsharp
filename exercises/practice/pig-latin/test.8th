"pig-latin.8th" f:include
needs exercism/test
with: test
22 tests

"Word beginning with a"
    expect_equal(translate("apple"
    "appleay"
    equal?)

SKIP-REST-OF-TESTS

"Word beginning with e"
    expect_equal(translate("ear"
    "earay"
    equal?)

"Word beginning with i"
    expect_equal(translate("igloo"
    "iglooay"
    equal?)

"Word beginning with o"
    expect_equal(translate("object"
    "objectay"
    equal?)

"Word beginning with u"
    expect_equal(translate("under"
    "underay"
    equal?)

"Word beginning with a vowel and followed by a qu"
    expect_equal(translate("equal"
    "equalay"
    equal?)

"Word beginning with p"
    expect_equal(translate("pig"
    "igpay"
    equal?)

"Word beginning with k"
    expect_equal(translate("koala"
    "oalakay"
    equal?)

"Word beginning with x"
    expect_equal(translate("xenon"
    "enonxay"
    equal?)

"Word beginning with q without a following u"
    expect_equal(translate("qat"
    "atqay"
    equal?)

"Word beginning with ch"
    expect_equal(translate("chair"
    "airchay"
    equal?)

"Word beginning with qu"
    expect_equal(translate("queen"
    "eenquay"
    equal?)

"Word beginning with qu and a preceding consonant"
    expect_equal(translate("square"
    "aresquay"
    equal?)

"Word beginning with th"
    expect_equal(translate("therapy"
    "erapythay"
    equal?)

"Word beginning with thr"
    expect_equal(translate("thrush"
    "ushthray"
    equal?)

"Word beginning with sch"
    expect_equal(translate("school"
    "oolschay"
    equal?)

"Word beginning with yt"
    expect_equal(translate("yttria"
    "yttriaay"
    equal?)

"Word beginning with xr"
    expect_equal(translate("xray"
    "xrayay"
    equal?)

"Y is treated like a consonant at the beginning of a word"
    expect_equal(translate("yellow"
    "ellowyay"
    equal?)

"Y is treated like a vowel at the end of a consonant cluster"
    expect_equal(translate("rhythm"
    "ythmrhay"
    equal?)

"Y as second letter in two letter word"
    expect_equal(translate("my"
    "ymay"
    equal?)

"A whole phrase"
    ( "quick fast run" translate )
    "ickquay astfay unray"
    equal?
