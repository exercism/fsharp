"wordy.8th" f:include
needs exercism/test
with: test
23 tests

"Just a number"
  expect_equal(answer "What is 5?", 5)

SKIP-REST-OF-TESTS

"Addition"
  expect_equal(answer "What is 1 plus 1?", 2)

"More addition"
  expect_equal(answer "What is 53 plus 2?", 55)

"Addition with negative numbers"
  expect_equal(answer "What is -1 plus -10?", -11)

"Large addition"
  expect_equal(answer "What is 123 plus 45678?", 45801)

"Subtraction"
  expect_equal(answer "What is 4 minus -12?", 16)

"Multiplication"
  expect_equal(answer "What is -3 multiplied by 25?", -75)

"Division"
  expect_equal(answer "What is 33 divided by -3?", -11)

"Multiple additions"
  expect_equal(answer "What is 1 plus 1 plus 1?", 3)

"Addition and subtraction"
  expect_equal(answer "What is 1 plus 5 minus -2?", 8)

"Multiple subtraction"
  expect_equal(answer "What is 20 minus 4 minus 13?", 3)

"Subtraction then addition"
  expect_equal(answer "What is 17 minus 6 plus 3?", 14)

"Multiple multiplication"
  expect_equal(answer "What is 2 multiplied by -2 multiplied by 3?", -12)

"Addition and multiplication"
  expect_equal(answer "What is -3 plus 7 multiplied by -2?", -8)

"Multiple division"
  expect_equal(answer "What is -12 divided by 2 divided by -3?", 2)

"Unknown operation"
  expect_equal(answer "What is 52 cubed?", None)

"Non math question"
  expect_equal(answer "Who is the President of the United States?", None)

"Reject problem missing an operand"
  expect_equal(answer "What is 1 plus?", None)

"Reject problem with no operands or operators"
  expect_equal(answer "What is?", None)

"Reject two operations in a row"
  expect_equal(answer "What is 1 plus plus 2?", None)

"Reject two numbers in a row"
  expect_equal(answer "What is 1 plus 2 1?", None)

"Reject postfix notation"
  expect_equal(answer "What is 1 2 plus?", None)

"Reject prefix notation"
  expect_equal(answer "What is plus 1 2?", None)

end-of-tests
;with
