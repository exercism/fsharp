"acronym.8th" f:include
needs exercism/test
with: test
9 tests

"Basic"
    ( "Portable Network Graphics" abbreviate )
    "PNG"
    equal?

SKIP-REST-OF-TESTS

"Lowercase words"
    ( "Ruby on Rails" abbreviate )
    "ROR"
    equal?

"Punctuation"
    ( "First In, First Out" abbreviate )
    "FIFO"
    equal?

"All caps word"
    ( "GNU Image Manipulation Program" abbreviate )
    "GIMP"
    equal?

"Punctuation without whitespace"
    ( "Complementary metal-oxide semiconductor" abbreviate )
    "CMOS"
    equal?

"Very long abbreviation"
    ( "Rolling On The Floor Laughing So Hard That My Dogs Came Over And Licked Me" abbreviate )
    "ROTFLSHTMDCOALM"
    equal?

"Consecutive delimiters"
    ( "Something - I made up from thin air" abbreviate )
    "SIMUFTA"
    equal?

"Apostrophes"
    ( "Halley's Comet" abbreviate )
    "HC"
    equal?

"Underscore emphasis"
    ( "The Road _Not_ Taken" abbreviate )
    "TRNT"
    equal?
