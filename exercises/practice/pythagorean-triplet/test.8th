"pythagorean-triplet.8th" f:include
needs exercism/test
with: test
7 tests

"Triplets whose sum is 12"
    ( 12 tripletsWithSum )
    c((3, 4, 5
    equal))

SKIP-REST-OF-TESTS

"Triplets whose sum is 108"
    ( 108 tripletsWithSum )
    c((27, 36, 45
    equal))

"Triplets whose sum is 1000"
    ( 1000 tripletsWithSum )
    c((200, 375, 425
    equal))

"No matching triplets for 1001"
    tripletsWithSum 1001 |> should be Empty

"Returns all matching triplets"
    ( 90 tripletsWithSum )
    c((9, 40, 41
    equal, (15, 36, 39)))

"Several matching triplets"
    ( 840 tripletsWithSum )
    c((40, 399, 401
    equal, (56, 390, 394), (105, 360, 375), (120, 350, 370), (140, 336, 364), (168, 315, 357), (210, 280, 350), (240, 252, 348)))

"Triplets for large number"
    ( 30000 tripletsWithSum )
    c((1200, 14375, 14425
    equal, (1875, 14000, 14125), (5000, 12000, 13000), (6000, 11250, 12750), (7500, 10000, 12500)))
