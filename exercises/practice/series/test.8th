"series.8th" f:include
needs exercism/test
with: test
11 tests

"Slices of one from one"
  expect_equal(slices "1" 1, ["1"])

SKIP-REST-OF-TESTS

"Slices of one from two"
    expect_equal(slices "12" 1, (Some c("1"
    "2"
    equal?))

"Slices of two"
  expect_equal(slices "35" 2, ["35"])

"Slices of two overlap"
    expect_equal(slices "9142" 2, (Some c("91", "14"
    "42"
    equal?))

"Slices can include duplicates"
    expect_equal(slices "777777" 3, (Some c("777", "777", "777"
    "777"
    equal?))

"Slices of a long series"
    expect_equal(slices "918493904243" 5, (Some c("91849", "18493", "84939", "49390", "93904", "39042", "90424"
    "04243"
    equal?))

"Slice length is too large"
  expect_equal(slices "12345" 6, None)

"Slice length is way too large"
  expect_equal(slices "12345" 42, None)

"Slice length cannot be zero"
  expect_equal(slices "12345" 0, None)

"Slice length cannot be negative"
  expect_equal(slices "123" -1, None)

"Empty series is invalid"
  expect_equal(slices "" 1, None)
